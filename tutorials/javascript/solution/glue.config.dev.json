{
    "glueAssets": {
        "worker": "./node_modules/@glue42/worker-web/dist/worker.js",
        "gateway": {
            "location": "./node_modules/@glue42/gateway-web/web/gateway-web.js"
        },
        "config": "./glue.config.json",
        "layouts": "./glue.layouts.json",
        "route": "/glue",
        "workspaces": {
            "appLocation": "./node_modules/@glue42/workspaces-app",
            "manifestLocation": "./workspaces.webmanifest"
        }
    },
    "server": {
        "apps": [
            {
                "route": "/",
                "file": {
                    "path": "./"
                }
            },
            {
                "route": "/clients",
                "file": {
                    "path": "./Clients/"
                }
            },
            {
                "route": "/stocks",
                "file": {
                    "path": "./Stocks/"
                }
            },
            {
                "route": "/clientdetails",
                "file": {
                    "path": "./client-details/"
                }
            }
        ],
        "settings": {
            "port": 4242,
            "disableCache": true
        },
        "sharedAssets": [
            {
                "path": "./assets/",
                "route": "/assets"
            },
            {
                "path": "./lib/",
                "route": "/lib"
            },
            {
                "path": "./favicon.ico",
                "route": "/favicon.ico"
            },
            {
                "path": "./service-worker.js",
                "route": "/service-worker.js"
            }
        ]
    },
    "logging": "dev"
}